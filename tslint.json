{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "member-access": false,
    "prettier": [true, {
      "printWidth": 100,
      "semi": false,
      "useTabs": false,
      "singleQuote": true,
      "trailingComma": "es5",
      "bracketSpacing": true
    }]
  }
}
